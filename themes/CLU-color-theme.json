{
   "name": "Tron Legacy Theme",
   "type": "dark",
   "colors": {
      "editor.background": "#000000",
      "editor.foreground": "#E0F7FA",
      "editorCursor.foreground": "#00FFFF",
      "editor.lineHighlightBackground": "#bcb90643",
      "editor.selectionBackground": "#014a4f",

      "editor.inactiveSelectionBackground": "#003B5C80",
      "editorIndentGuide.background": "#2E2E2E",
      "editorIndentGuide.activeBackground": "#5A5A5A",
      "activityBar.background": "#000000",
      "sideBar.background": "#000000",
      "statusBar.background": "#00121c",
      "statusBar.foreground": "#E0F7FA",
      "titleBar.activeBackground": "#000000",
      "titleBar.activeForeground": "#00FFFF",
      "button.background": "#1DE9F6",
      "button.foreground": "#000000",
      "button.hoverBackground": "#0019f7"
   },
   "tokenColors": [
      {
         "scope": ["keyword", "storage.type", "storage.modifier"],
         "settings": {
            "foreground": "#00FFFF",
            "fontStyle": "bold"
         }
      },
      {
         "scope": ["string", "constant.other.symbol"],
         "settings": {
            "foreground": "#FFF176"
         }
      },
      {
         "scope": ["variable", "variable.parameter"],
         "settings": {
            "foreground": "#E0F7FA"
         }
      },
      {
         "scope": ["comment", "punctuation.definition.comment"],
         "settings": {
            "foreground": "#dadada"
         }
      },
      {
         "scope": ["function", "entity.name.function"],
         "settings": {
            "foreground": "#00FF94"
         }
      },
      {
         "scope": ["constant.numeric"],
         "settings": {
            "foreground": "#00FF94"
         }
      },
      {
         "scope": ["invalid", "invalid.illegal"],
         "settings": {
            "foreground": "#FF3B3B",
            "fontStyle": "underline"
         }
      }
   ]
}
